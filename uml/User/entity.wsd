@startuml

class User {
  + id: uuid
  + nickname: String
  + email: String
  + password: String
  + emailVerified: boolean
  + createdAt: Date
  + updatedAt: Date
  + trainings: Training[]
  + newPasswordCodes: NewPasswordCode[]
  + workoutHistory: WorkoutHistory[]
  + goals: Goals[]
  + bodyData: BodyData[]
  + performanceEvaluations: PerformanceEvaluation[]
}

@enduml